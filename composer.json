{
	"name" : "coka/rest-request-validator-bundle",
	"type" : "symfony-bundle",
	"description" : "Help the user input high quality data into your web services REST",
	"license" : "MIT",
	"keywords" : [
		"REST",
		"Input Validation",
		"Input Filtering",
		"Validate Content types"
	],
	"authors" : [{
			"name" : "Cedrick Oka Baidai",
			"email" : "okacedrick@gmail.com"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Oka\\RESTRequestValidatorBundle\\" : "src"
		},
		"exclude-from-classmap" : [
			"/tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\RESTRequestValidatorBundle\\Tests\\" : "tests/"
		}
	},
	"require" : {
		"php" : ">=7.2",
		"doctrine/annotations" : "^1.6",
		"doctrine/cache" : "^1.8",
		"symfony/config" : "^3.4 || ^4.2",
		"symfony/dependency-injection" : "^3.4 || ^4.2",
		"symfony/doctrine-bridge" : "^3.4 || ^4.2",
		"symfony/http-kernel" : "^3.4 || ^4.2",
		"symfony/security-core" : "^3.4 || ^4.2",
		"symfony/translation" : "^3.4 || ^4.2",
		"symfony/validator" : "^3.4 || ^4.2",
		"symfony/serializer" : "^3.4 || ^4.2",
		"symfony/framework-bundle" : "^3.4 || ^4.2"
	},
	"require-dev" : {
		"jms/serializer-bundle" : "^2.3",
		"symfony/form" : "^3.4 || ^4.2",
		"symfony/phpunit-bridge" : "^3.4 || ^4.2"
	},
	"suggest" : {
		"jms/serializer-bundle" : "^2.3",
		"symfony/form" : "^3.4 || ^4.2"
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.x.x-dev"
		},
		"symfony" : {
			"allow-contrib" : "true"
		}
	},
	"support" : {
		"docs" : "https://github.com/CedrickOka/rest-request-validator-bundle/blob/master/src/Resources/doc/index.md"
	}
}
